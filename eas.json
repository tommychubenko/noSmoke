{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
      // Помилкове налаштування "android" видалено звідси
    }
  },
  "submit": {
    "production": {
      // ПРАВИЛЬНЕ РОЗМІЩЕННЯ: "autoUpload" переміщено в секцію "submit"
      "android": {
        "autoUpload": true
      }
    }
  }
}